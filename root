<!-- Lightweight JS: year + (optional) RSS integration for static hosting like GoDaddy -->
<script>
  // Auto year
  document.getElementById("year").textContent = new Date().getFullYear();

  // Optional: Simple RSS loader using rss2json for up to 3 sources.
  // 1) Add RSS URLs as data-rss-* on #local-news-list.
  // 2) This will append top headlines under Local News Feed.
  (function() {
    var list = document.getElementById("local-news-list");
    if (!list) return;

    var rssUrls = [
      list.getAttribute("data-rss-1"),
      list.getAttribute("data-rss-2"),
      list.getAttribute("data-rss-3")
    ].filter(Boolean);

    if (!rssUrls.length) return;

    rssUrls.forEach(function(url) {
      var apiUrl = "https://api.rss2json.com/v1/api.json?rss_url=" + encodeURIComponent(url);
      fetch(apiUrl)
        .then(function(res) { return res.json(); })
        .then(function(data) {
          if (!data || !data.items) return;
          data.items.slice(0, 3).forEach(function(item) {
            var li = document.createElement("li");
            var a = document.createElement("a");
            a.href = item.link;
            a.target = "_blank";
            a.rel = "noopener";
            a.innerHTML = '<span class="dot"></span>' + (item.title || "View update");
            li.appendChild(a);
            list.appendChild(li);
          });
        })
        .catch(function() {
          // Fail silently; base static links remain.
        });
    });
  })();
</script>
</body>
</html>
